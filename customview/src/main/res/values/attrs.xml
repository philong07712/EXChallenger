<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CornerImageView">
        <attr name="topLeftRadius" format="dimension" />
        <attr name="topRightRadius" format="dimension" />
        <attr name="bottomRightRadius" format="dimension" />
        <attr name="bottomLeftRadius" format="dimension" />
    </declare-styleable>

    <declare-styleable name="CustomTextviewFonts">
        <attr name="font_type" format="string" />
    </declare-styleable>
    <declare-styleable name="SearchBarBehavior">
        <attr name="margin_default" format="dimension" />
    </declare-styleable>
    <declare-styleable name="ExpandableTextViewFonts">
        <attr name="trimLength" format="integer"/>
        <attr name="appendTextColor" format="color"/>
        <attr name="appendText" format="string"/>
    </declare-styleable>

    <!--FONT-->
    <string name="bold">fonts/Nunito-Bold.ttf</string>
    <string name="medium">fonts/Nunito-SemiBold.ttf</string>
    <string name="italic">fonts/Nunito-Italic.ttf</string>
    <string name="regular">fonts/Nunito-Regular.ttf</string>

    <string name="roboto_bold">fonts/Roboto-Bold.ttf</string>
    <string name="roboto_medium">fonts/Roboto-Medium.ttf</string>
    <string name="roboto_regular">fonts/Roboto-Regular.ttf</string>

    <!--END-->


</resources>